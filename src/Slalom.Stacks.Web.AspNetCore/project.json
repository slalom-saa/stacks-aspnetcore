    "Microsoft.AspNetCore.Mvc": "1.1.2",
    "Microsoft.AspNetCore.Mvc.ApiExplorer": "1.1.2",
    "Microsoft.AspNetCore.Mvc.Formatters.Json": "1.1.2",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.1",
