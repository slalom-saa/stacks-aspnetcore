{
  "version": "1.5.0-*",
  "authors": [
    "Slalom Architect Academy"
  ],
  "packOptions": {
    "tags": [
      "slalom",
      "stacks",
      "aspnetcore",
      "blocks"
    ],
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
