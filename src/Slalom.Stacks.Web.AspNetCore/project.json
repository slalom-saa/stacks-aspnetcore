    "Microsoft.Extensions.FileProviders.Embedded": "1.0.1",
    "Microsoft.AspNetCore.StaticFiles": "1.0.2",
    "Slalom.Stacks": "1.5.3"
  },
  "frameworks": {
    "net461": {
      "dependencies": {
        
      },
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "embed": {
      "include": [
        "bower_components/swagger-ui/dist/**/*.*",
        "Swagger/Swashbuckle.AspNetCore.SwaggerUI/Template/index.html"
      ]
    },
    "additionalArguments": [
      "/ruleset:../../assets/stacks.ruleset",
      "/additionalfile:../../assets/stylecop.json"
    ],
    "xmlDoc": true
  }
