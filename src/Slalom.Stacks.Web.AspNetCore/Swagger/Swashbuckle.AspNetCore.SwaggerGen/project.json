{
  "version": "1.0.0-*",
  "description": "Swagger Generator for API's built on ASP.NET Core",

  "buildOptions": {
    "keyFile": "Swashbuckle.AspNetCore.SwaggerGen.snk",
    "xmlDoc": true,
    "nowarn": [ "1591" ]
  },

  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/domaindrivendev/Swashbuckle.AspNetCore/master/LICENSE",
    "owners": [
      "Richard Morris"
    ],
    "projectUrl": "https://github.com/domaindrivendev/Swashbuckle.AspNetCore",
    "summary": "Includes an implementation of ISwaggerProvider that automatically generates SwaggerDocument(s) from routes, controllers and models",
    "tags": [
      "swagger",
      "documentation",
      "discovery",
      "help",
      "webapi",
      "aspnet",
      "aspnetcore"
    ]
  },

  "dependencies": {
    "Microsoft.AspNetCore.Mvc.ApiExplorer": "1.0.0",
    "Microsoft.AspNetCore.Mvc.Core": "1.0.0",
    "Microsoft.AspNetCore.Mvc.DataAnnotations": "1.0.0",
    "Swashbuckle.AspNetCore.Swagger": "1.0.0-*"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Xml.XPath": "4.0.1"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  }
}